    <script type="text/javascript">
        function companvalidate(name) {

            var returnFormStatus = true;
            var returnEmptyStatus = formEmptyStatus('.company');

            returnFormStatus = (returnEmptyStatus == 'invalid') ? false : returnFormStatus;

            if (!returnFormStatus) {

                showAlrt(name);

                returnFormStatus = false;
            }

            return returnFormStatus;
        }

        function formEmptyStatus(formId) {

            var formStatus = 'valid';
            formStatus = 'invalid';


            return formStatus;

        }

        /* validation related alert messages */
        function showAlrt(name) {
            var numbersArray = name.split('-->');
            $.each(numbersArray, function (index, value) {
                if (value != '') {
                    toastr.error(value);
                }
            });

        }

        function remAlrt() {
            $('.alrt-div .hide').remove();
            //$('.alrt-div').delay(2000).fadeOut('slow');
        }
    </script>
